import streamlit as st
import pandas as pd
import numpy as np
# from bokeh.plotting import figure, show
import matplotlib.pyplot as plt
# import streamlit_aggrid as ag

st.set_page_config(
    page_title="Dashboard",
    page_icon="ðŸ‘‹",

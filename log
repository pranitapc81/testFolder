import pandas as pd

# Sample DataFrame
data = {'key': [1, 2, 3, 4, 5],
        'overall_status': ['pass', 'fail', 'pass', 'fail', 'pass']}
df = pd.DataFrame(data)

# Count and filter rows with 'pass' status
pass_count = df[df['overall_status'] == 'pass'].shape[0]
pass_data = df[df['overall_status'] == 'pass']

# Count and filter rows with 'fail' status
fail_count = df[df['overall_status'] == 'fail'].shape[0]
fail_data = df[df['overall_status'] == 'fail']

print("Number of rows with 'pass' status:", pass_count)
print("Data with 'pass' status:")
print(pass_data)

print("Number of rows with 'fail' status:", fail_count)
print("Data with 'fail' status:")
print(fail_data)

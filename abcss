import streamlit as st
import pandas as pd

# Load the CSV data
data = pd.read_csv('your_data.csv')

# Function to display collapsible sections
def display_collapsible_section(row):
    with st.beta_expander(f"Row: {row.name}", expanded=False):
        for column in data.columns:
            if column.startswith("expected_") or column.startswith("actual_") or column.startswith("result_"):
                st.write(f"{column}: {row[column]}")

# Main Streamlit app
st.title('Data Display App')
st.write('Displaying data with collapsible sections for "expected," "actual," and "result" fields.')

# Display data
for index, row in data.iterrows():
    display_collapsible_section(row)

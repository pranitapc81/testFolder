from datetime import datetime

def get_current_datetime_formatted_microseconds():
    # Get the current datetime
    now = datetime.now()
    
    # Format the datetime to include year, month, day, hour, minute, second, and microseconds
    formatted_datetime = now.strftime("%y%m%d%H%M%S%f")
    
    return formatted_datetime

# Example usage
formatted_datetime = get_current_datetime_formatted_microseconds()
print(formatted_datetime)

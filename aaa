import pandas as pd

def clean_dataframe(df):
    """
    This function removes specific columns and drops duplicate rows from the DataFrame.
    
    Parameters:
    df (pd.DataFrame): The input DataFrame to be cleaned.
    
    Returns:
    pd.DataFrame: The cleaned DataFrame with specified columns removed and duplicates dropped.
    """
    # Columns to be removed
    columns_to_remove = ['Path', 'Value', 'RuleID', 'GeneratedTradeID']
    
    # Remove the specified columns if they exist in the DataFrame
    df_cleaned = df.drop(columns=[col for col in columns_to_remove if col in df.columns])
    
    # Drop duplicate rows
    df_cleaned = df_cleaned.drop_duplicates()
    
    return df_cleaned

# Sample DataFrame for demonstration
data = {
    'Column1': [1, 2, 3, 2],
    'Path': ['A', 'B', 'C', 'B'],
    'Value': [10, 20, 30, 20],
    'RuleID': [100, 200, 300, 200],
    'GeneratedTradeID': [1000, 2000, 3000, 2000],
    'Column2': ['X', 'Y', 'Z', 'Y']
}

df = pd.DataFrame(data)

# Use the function to clean the DataFrame
df_cleaned = clean_dataframe(df)

# Display the cleaned DataFrame
print(df_cleaned)
